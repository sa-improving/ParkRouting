@{
}
<h1>Parks</h1>
<div>
    @foreach(var park in Model.ParksListed)
            {
                <br />
                <h3>@park.Parkname</h3>
                <p>@park.Borough</p>
                <p>Acres: @park.Acres</p>
                <p>@Html.Raw(park.Description)</p>
            }
</div>